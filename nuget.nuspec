<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <version>1.13.5</version>
        <id>datatables.net-bs4</id>
        <description>This is DataTables with styling for [Bootstrap4](https://getbootstrap.com/docs/4.6/getting-started/introduction/)</description>
        <repository type="git" url="https://github.com/DataTables/Dist-DataTables-Bootstrap4.git"/>
        <authors>SpryMedia Ltd</authors>
        <projectUrl>http://datatables.net</projectUrl>
        <license type="expression">MIT</license>
        <tags>Bootstrap  Bootstrap4  Datatables jQuery table</tags>
        <dependencies>
            <dependency id="datatables.net" version="1.12.1"/>
            <dependency id="bootstrap" version="4.*"/>
        </dependencies>
        <contentFiles>
            <files include="**/*" buildAction="Content"/>
        </contentFiles>
    </metadata>
    <files>
        <file src="js/dataTables.bootstrap4.js" target="content\Scripts"/>
        <file src="js/dataTables.bootstrap4.js" target="contentFiles\any\any\wwwroot\js"/>
        <file src="js/dataTables.bootstrap4.min.js" target="content\Scripts"/>
        <file src="js/dataTables.bootstrap4.min.js" target="contentFiles\any\any\wwwroot\js"/>
        <file src="css/dataTables.bootstrap4.css" target="content\Content"/>
        <file src="css/dataTables.bootstrap4.css" target="contentFiles\any\any\wwwroot\css"/>
        <file src="css/dataTables.bootstrap4.min.css" target="content\Content"/>
        <file src="css/dataTables.bootstrap4.min.css" target="contentFiles\any\any\wwwroot\css"/>
    </files>
</package>

